fnjs_distr_cr=function() {
    var ok=$('#bypass').val();
    if (ok == 't') {
      $("#dst_cr").show();
    } else {
      $("#dst_cr").hide();
    }
}

fnjs_grupo=function() {
    var ok=$('#grupo_dst').prop('checked')
    if (ok) {
      $("#dst_array").hide();
      $("#dst_grupo").show();
    } else {
      $("#dst_array").show();
      $("#dst_grupo").hide();
    }
}

