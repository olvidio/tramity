{% if secretaria %}
<button type="button" class="btn btn-info" onClick="fnjs_update_div('#main','{{ pagina_nueva|raw }}');">
	{{ "nueva entrada"|trans }}
</button>
{% endif %}
<table class="table table-sm table-bordered">
  <thead class="thead-light">
    <tr>
      <th scope="col">#</th>
      <th scope="col">{{ "protocolo"|trans }}</th>
      <th scope="col">{{ "org./dest."|trans }}</th>
      <th scope="col">{{ "ref."|trans }}</th>
      <th scope="col">{{ "categoria"|trans }}</th>
      <th scope="col">{{ "asunto"|trans }}</th>
      <th scope="col">{{ "oficinas"|trans }}</th>
      <th scope="col">{{ "fecha escrito"|trans }}</th>
      <th scope="col">{{ "contestar antes de"|trans }}</th>
    </tr>
  </thead>
  <tbody>
  {% for row in a_entradas %}
    <tr>
      <th scope="row">{{ row.link_mod|raw }}</th>
      <td>{{ row.protocolo }}</td>
      <td>{{ row.link_ver|raw }}</td>
      <td>{{ row.referencias|raw }}</td>
      <td>{{ row.categoria }}</td>
      <td>{{ row.asunto }}</td>
      <td>{{ row.oficinas|raw }}</td>
      <td>{{ row.f_escrito }}</td>
      <td>{{ row.f_contestar }}</td>
    </tr>
    {% endfor %}
  </tbody>
</table>
<script type="text/javascript">
$(document).ready(function() {
	tabs_show('{{ vista }}','{{ filtro }}');
});
fnjs_ver_entrada=function(id_entrada) {
    var ssfsv = 'sv';
    var winPrefs="dependent=yes,width=950,height=700,screenX=200,screenY=200,titlebar=yes,scrollbars=yes";
    var server = '{{ server }}'
    var url = server+'/apps/entradas/controller/entrada_ver.php?id_entrada='+id_entrada;
    w=window.open(url, "sele", winPrefs);
    w.focus();
}
</script>